[application]

config/name="1- Bit Jumping"
config/description="Reality... behaves strangely in these caverns. People never seem to age and dead creatures come back to life. Rock ledges age and crumble to dust within seconds, and then sometimes within minutes reform back to their original state."
run/main_scene="res://scenes/splash_screen/SplashScreen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Talltale Studios/1-Bit Jumping"
boot_splash/image="res://assets/graphics/black_bootsplash.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://assets/graphics/game_icon.png"

[autoload]

Loader="*res://autoload/interactive_loader/InteractiveLoader.tscn"
Global="*res://autoload/Globals.gd"
egs="*res://addons/ExtraGui/extraGuiSingleton.gd"
AntialiasedLine2DTexture="*res://addons/antialiased_line2d/texture.gd"

[display]

window/size/width=1600
window/size/height=900
window/dpi/allow_hidpi=true
window/vsync/vsync_via_compositor=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/ExtraGui/plugin.cfg", "res://addons/animation_frame_picker/plugin.cfg", "res://addons/antialiased_line2d/plugin.cfg", "res://addons/branch/plugin.cfg", "res://addons/epic-anchors/plugin.cfg", "res://addons/launchy/plugin.cfg", "res://addons/mixing-desk/plugin.cfg", "res://addons/orphan_import_file_cleaner/plugin.cfg", "res://addons/project_map/plugin.cfg", "res://addons/sprite_quick_offset/plugin.cfg", "res://addons/zoomy/plugin.cfg" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 2,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": true,
"flags/repeat": true,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

